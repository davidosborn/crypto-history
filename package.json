{
	"name": "@davidosborn/crypto-history",
	"version": "0.0.8",
	"description": "Crypto History",
	"keywords": [
		"crypto",
		"history"
	],
	"homepage": "https://github.com/davidosborn/crypto-history#readme",
	"bugs": {
		"url": "https://github.com/davidosborn/crypto-history/issues",
		"email": "david@davidosborn.io"
	},
	"license": "MIT",
	"author": {
		"name": "David Osborn",
		"email": "david@davidosborn.io",
		"url": "http://davidosborn.io/"
	},
	"files": [
		"lib"
	],
	"main": "index.js",
	"bin": "index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/davidosborn/crypto-history"
	},
	"scripts": {
		"build": "npx babel -d lib src",
		"prepublishOnly": "npm run build",
		"start": "node index",
		"start-background": "npx pm2 start --name=crypto-history index",
		"restart-background": "npx pm2 restart crypto-history",
		"stop-background": "npx pm2 stop crypto-history",
		"delete-background": "npx pm2 delete crypto-history"
	},
	"config": {
		"db": "crypto_history",
		"log_scheduler": true,
		"log_sql": true,
		"update_history_task_interval": 3600000
	},
	"dependencies": {
		"@davidosborn/api-framework": "*",
		"@davidosborn/scheduler": "*",
		"@davidosborn/sql-utilities": "*",
		"node-fetch": "*"
	},
	"devDependencies": {
		"@babel/cli": "^7.0.0-beta",
		"@babel/core": "^7.0.0-beta",
		"@babel/plugin-proposal-optional-chaining": "^7.0.0-beta",
		"@babel/preset-env": "^7.0.0-beta",
		"@babel/preset-stage-0": "^7.0.0-beta",
		"@babel/register": "^7.0.0-beta",
		"babel-preset-minify": "canary",
		"pm2": "*"
	},
	"publishConfig": {
		"access": "public"
	},
	"babel": {
		"presets": [
			[
				"@babel/env",
				{
					"targets": {
						"node": "current"
					}
				}
			],
			"@babel/stage-0",
			"minify"
		],
		"plugins": [
			"@babel/proposal-optional-chaining"
		]
	}
}
