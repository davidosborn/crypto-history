{
	"name": "crypto-history",
	"version": "0.0.0",
	"description": "Crypto History",
	"keywords": [
		"crypto",
		"history"
	],
	"homepage": "https://github.com/davidosborn/crypto-history",
	"bugs": {
		"url": "https://github.com/davidosborn/crypto-history",
		"email": "david@davidosborn.io"
	},
	"license": "MIT",
	"author": {
		"name": "David Osborn",
		"email": "david@davidosborn.io",
		"url": "http://davidosborn.io/"
	},
	"main": "lib/index.js",
	"bin": "lib/index.js",
	"dependencies": {
		"@davidosborn/api-framework": "*",
		"@davidosborn/scheduler": "*",
		"@davidosborn/sql-utilities": "*",
		"node-fetch": "*"
	},
	"devDependencies": {
		"@babel/cli": "^7.0.0-beta",
		"@babel/core": "^7.0.0-beta",
		"@babel/node": "^7.0.0-beta",
		"@babel/plugin-proposal-optional-chaining": "^7.0.0-beta",
		"@babel/preset-env": "^7.0.0-beta",
		"@babel/preset-stage-0": "^7.0.0-beta",
		"babel-preset-minify": "canary"
	},
	"scripts": {
		"build": "npx babel -d lib src",
		"prepublish": "npm run build",
		"start": "node --harmony index",
		"start-forever": "forever start -a --minUptime 10000 --uid crypto-history -c 'node --harmony' index.js",
		"stop-forever": "forever stop crypto-history"
	},
	"config": {
		"db": "crypto_history",
		"log_scheduler": true,
		"log_sql": true
	},
	"babel": {
		"presets": [
			[
				"@babel/env", {
					"targets": {
						"node": "current"
					}
				}
			],
			"@babel/stage-0",
			"minify"
		],
		"plugins": [
			"@babel/proposal-optional-chaining"
		]
	}
}
